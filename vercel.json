{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/ws/(.*)",
        "dest": "/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/server.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }